<project xmlns="http://maven.apache.org/POM/4.0.0" 
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
  
	<parent>
		<groupId>net.imagej</groupId>
		<artifactId>pom-imagej</artifactId>
		<version>15.1.0</version>
		<relativePath />
	</parent>

	<groupId>com.github.jonmarsh</groupId>
	<artifactId>waveform_processing_for_imagej</artifactId>
	<version>0.1.0-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>Waveform Processing for ImageJ</name>
	<description>A Maven project implementing ImageJ plugins for processing waveforms</description>
	<url>http://maven.apache.org</url>
  
	<developers>
		<developer>
			<name>Jon N. Marsh</name>
		</developer>
	</developers>
	
	<scm>
		<connection>scm:git:git://github.com/jonmarsh/waveform_processing_for_imagej</connection>
		<developerConnection>scm:git:git@github.com:jonmarsh/waveform_processing_for_imagej</developerConnection>
		<tag>HEAD</tag>
		<url>https://github.com/jonmarsh/waveform_processing_for_imagej</url>
	</scm>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>

	<dependencies>
		<dependency>
			<groupId>net.imagej</groupId>
			<artifactId>ij</artifactId>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>
	
	
</project>
